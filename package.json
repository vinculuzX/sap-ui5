    "@openui5/themelib_sap_belize": "^1.60.0"
  },
  "devDependencies": {
    "@ui5/cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "eslint": "^4.19.1",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-openui5": "^0.2.3",
    "karma-qunit": "^1.2.1",
    "qunitjs": "^2.4.1",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.4.1"
